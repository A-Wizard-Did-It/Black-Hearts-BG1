BEGIN WIYUJEHL

CHAIN IF ~NumTimesTalkedTo(0)~ THEN WIYUJEHL WIJELLY1
	@3297
END
		++ @3298 + WIJELLY2
		++ @3299 + WIJELLY3
		++ @3300 + WIJELLY4
		
CHAIN WIYUJEHL WIJELLY2
	@3301 EXIT
	
CHAIN WIYUJEHL WIJELLY3
	@3302 EXIT

CHAIN WIYUJEHL WIJELLY4
	@3303 DO ~EscapeArea()~ EXIT
	
CHAIN IF ~NumTimesTalkedToGT(0) !Dead("WIPINATA")~ THEN WIYUJEHL WIJELLY5
	@3304 EXIT
	
CHAIN IF ~NumTimesTalkedToGT(0) Dead("WIPINATA")~ THEN WIYUJEHL WIJELLY6
	@3305 DO ~EscapeArea()~ EXIT