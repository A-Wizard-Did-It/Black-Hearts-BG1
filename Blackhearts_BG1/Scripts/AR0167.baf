	DisplayStringHead("WINerys",~Ok, got them!~)
	Wait(1)
	ActionOverride("WINerys",MoveToPoint([315.285]))
	Wait(2)
	Face(0)
	EndCutSceneMode()
	ActionOverride("WINerys",StartDialogueNoSet(Player1))
END
