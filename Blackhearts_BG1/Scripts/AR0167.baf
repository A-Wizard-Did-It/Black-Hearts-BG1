IF
  Global("WINerysMet","GLOBAL",2)
THEN
  RESPONSE #100
	StartCutSceneMode()
	SetGlobal("WINerysMet","GLOBAL",3)
	CutSceneId(Player1)
	Wait(2)
	DisplayStringHead("WINerys",~Now lets see here. I have all my Harper correspondences and records here. Gimmie a minute~)
